    <script>
        function addToCart(productId) {
            // You can customize the endpoint URL based on your server setup
            const addToCartUrl = '/cart/addtoCart';

            fetch(addToCartUrl, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({
                        productId,
                        quantity: 1
                    })
                })
                .then(response => response.json())
                .then(data => {
                    swal.fire({
                        title: "Add to cart successfull ",
                        icon: "success"
                    }).then((result)=>{
                        if(result.isConfirmed){
                            location.href='/cart'
                        }
                    })
                })
                .catch(error => {
                    swal.fire({
                        title: "Please login or sign in now ",
                        icon: "error"
                    })
                });
        }
        function addToWhislist(productId) {
            const addToWhislisttUrl = '/cart/addtoWhishlist';

            fetch(addToWhislisttUrl, {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({
                        productId,
                        quantity: 1
                    })
                })
                .then(response => response.json())
                .then(data => {
                    swal.fire({
                        title: "Add to Whislist successfull ",
                        icon: "success"
                    }).then((result)=>{
                        if(result.isConfirmed){
                            alert('SUCCESSFULL')
                        }
                    })
                })
                .catch(error => {
                    swal.fire({
                        title: "Please login or sign in now ",
                        icon: "error"
                    })
                });
        }
    </script>
